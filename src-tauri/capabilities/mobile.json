{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "mobile",
  "description": "Capability for main window on mobile (Android/iOS), app-events for back key",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "app-events:default",
    "dialog:default"
  ],
  "platforms": ["android", "iOS"]
}
